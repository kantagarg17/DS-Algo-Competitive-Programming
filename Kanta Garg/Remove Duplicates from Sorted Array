class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
        int len=0;
        if(nums.size()==0)
              return 0;
        for(int i=0;i<nums.size()-1;i++)
        {
            if(nums[i]!=nums[i+1]){
                nums[len]=nums[i];len++;
            }
        }
        nums[len]=nums[nums.size()-1];
        return (len+1);
        
    }
};
